{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (Gateway)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/Microservices/Gateway/2.WebApi/Demostrativo.Oeclot.WebApi/bin/Debug/net7.0/Demostrativo.Ocelot.WebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Microservices/Gateway/2.WebApi/Demostrativo.Ocelot.WebApi",
            "stopAtEntry": false,
            // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
